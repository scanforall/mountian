}

.page-content {
  padding: 60px 0 20px;
  background: url("../images/bg/admin.jpg") center center / cover no-repeat;
  flex: 1;
}

.title {
  margin-right: 60px;
  font-size: 21px;
  font-weight: bold;
  color: $text-color;
}